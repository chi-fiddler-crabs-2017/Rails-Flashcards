<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
    <li><%= error %></li>
    <%end %>
  </ul>
<%end %>


<%= form_for :user, url: users_path do |f| %>
<p>
<%= f.label :username %>
<%= f.text_field :username %>
</p>
<p>
<%= f.label :email %>
<%= f.text_field :email %>
</p>
<p>
<%= f.label :password %>
<%= f.password_field :password %>
</p>
<p>
  <%= f.submit "Register" %>
</p>
<% end %>
